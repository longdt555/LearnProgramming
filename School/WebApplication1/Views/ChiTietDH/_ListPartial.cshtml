@model List<ChiTietDHModel>

<table class="table tbl-contents">
    <tbody>
        @{
            var count = 0;
        }
        @foreach (var item in Model.ToList())
        {
            count++;
            <tr>
                <td class="w-5p">
                    @count
                </td>
                <td class="w-20p">
                    @item.CreatedDate
                </td>
                <td class="w-20p">
                    @item.UpdatedDate
                </td>
                <td class="w-15p">
                    @item.MaDH
                </td>
                <td class="w-10p">
                    @item.DonGia
                </td>
                <td class="w-15p">
                    @item.SoLuong
                </td>


                <td class="w-6p">
                    <a asp-action="Edit" asp-route-id="@item.Id"><i class="nc-icon nc-scissors"></i></a> |

                    <a href="#" onclick="deleteRecord(@item.Id)"><i class="nc-icon nc-simple-remove"></i></a>

                </td>
            </tr>
        }
    </tbody>
</table>
